{
  "name": "undiscord-cli",
  "nameFull": "Undiscord CLI",
  "version": "0.0.0",
  "description": "A fork of victornpb/undiscord adapted into a CLI",
  "license": "MIT",
  "engines": {
    "node": ">=18"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ayubun/undiscord-cli.git"
  },
  "homepage": "https://github.com/ayubun/undiscord-cli",
  "author": "ayubun",
  "bin": {
    "undiscord-cli": "src/cli.js"
  },
  "type": "module",
  "scripts": {
    "start": "npm run watch",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "cli": "node src/cli.js"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "delete",
    "messages",
    "discord",
    "wipe",
    "purge",
    "clean",
    "bulk",
    "cli",
    "selfbot"
  ],
  "devDependencies": {
    "eslint": "^8.36.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.0",
    "tiny-dedent": "^1.0.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**.js": "npm run lint"
  },
  "dependencies": {
    "chalk": "^5.6.2"
  }
}
